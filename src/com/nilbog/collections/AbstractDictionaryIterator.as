package com.nilbog.collections {	import com.nilbog.dbc.precondition;	import com.nilbog.errors.AbstractMethodCallError;	import com.nilbog.util.instantiatedAs;		import flash.utils.Dictionary;		/**	 * @author markhawley	 * 	 * Base dictionary-based collection iterator.	 */	internal class AbstractDictionaryIterator extends AbstractIterator	{		protected var impl:Dictionary;				/**		 * Constructor.		 * 		 * @param	collection	ICollection		 * @param	implementation	Dictionary		 * 		 * @throws	AbstractMethodCallError		 */		public function AbstractDictionaryIterator( collection:AbstractCollection, implementation:Dictionary )		{			precondition(collection !== null);			precondition(implementation !== null);						super(collection);						if (instantiatedAs(this, AbstractDictionaryIterator))			{				throw new AbstractMethodCallError("Cannot " +					"instantiate Abstract class.");			}						impl = implementation;		}	}}