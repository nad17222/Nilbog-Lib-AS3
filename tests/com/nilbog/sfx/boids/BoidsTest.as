package com.nilbog.sfx.boids {	import com.nilbog.util.Range;		import flash.display.Sprite;	import flash.events.Event;		/**	 * @author markhawley	 */	public class BoidsTest extends Sprite 	{		private var boids:BoidSystem;		public function BoidsTest()		{			boids = new BoidSystem();			addChild(boids);						// add boids to the system			new Range( 0, 10 ).each( function () :void			{				// boids start in random places with random				// starting values				var b:Boid = new Boid( "Boid1" );				b.x = -100 + Math.round(Math.random() * 100);				b.y = -100 + Math.round(Math.random() * 500);				var v:Number = 4;				b.xvelocity = 1 + v * Math.random();				b.yvelocity = .5 * v * (Math.random() - Math.random());				boids.addBoid(b);			});						stage.addEventListener(Event.ENTER_FRAME, onFrame);		}				private function onFrame( event:Event ) :void		{			boids.run();						// perform boid maintenance: create more boids, delete boids, etc.		}	}}